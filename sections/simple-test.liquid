<section class="simple-test">
  <h2>{{ section.settings.heading }}</h2>
  <p>{{ section.settings.text }}</p>
</section>

{% schema %}
  {
    "name": "Simple Test Section",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading",
        "default": "Hello from CLI"
      }, {
        "type": "text",
        "id": "text",
        "label": "Text",
        "default": "CLI section is working"
      }
    ],
    "presets": [
      {
        "name": "Simple Test Section"
      }
    ]
  }
{% endschema %}